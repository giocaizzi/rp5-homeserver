# N8N Stack Environment Variables
# Copy this file to .env and update with your actual values

# --- Required ---
# N8N Authentication: Set a strong password
N8N_BASIC_AUTH_PASSWORD=CHANGE_ME_TO_STRONG_PASSWORD

# PostgreSQL Database Configuration
POSTGRES_PASSWORD=CHANGE_ME_TO_STRONG_DB_PASSWORD

# --- Optional ---
# PostgreSQL Configuration
POSTGRES_USER=n8n
POSTGRES_DB=n8n

# N8N Host Configuration
# Set to your public domain for external access, defaults to n8n.local for local-only access
N8N_HOST=n8n.local

# User Configuration
TZ=UTC