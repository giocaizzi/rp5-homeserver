# Infrastructure Stack Environment Variables
# Copy this file to .env and update with your actual values

# Required
# ---------------------------
# Cloudflare Tunnel Configuration
CLOUDFLARED_TOKEN=your-cloudflare-tunnel-token-here

# Nginx Configuration
NGINX_CONF_PATH=./nginx/nginx.conf
NGINX_SSL_PATH=./nginx/ssl

# Backrest Backup Configuration
GCS_BUCKET_NAME=your-gcs-bucket-name
GCP_SERVICE_ACCOUNT_FILE=./backup/secrets/gcp_service_account.json

# Optional
# ---------------------------
# User Configuration
PUID=1000
PGID=1000
TZ=UTC